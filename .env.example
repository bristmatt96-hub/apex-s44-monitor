# Interactive Brokers
# Note: IB uses TWS or Gateway, no API keys needed - just connect to running instance

# Crypto Exchange (Binance)
CRYPTO_API_KEY=your_binance_api_key
CRYPTO_API_SECRET=your_binance_api_secret

# News API
NEWS_API_KEY=your_newsapi_key

# Alpha Vantage (market data)
ALPHA_VANTAGE_KEY=your_alpha_vantage_key

# Reddit (for sentiment)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret

# Twitter/X (for sentiment)
TWITTER_BEARER_TOKEN=your_twitter_bearer_token

# Redis (for agent communication)
REDIS_URL=redis://localhost:6379/0

# Database
DATABASE_URL=sqlite:///./trading.db
